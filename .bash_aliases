# builtins
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."
alias c="clear"
alias cl="clear"
alias l="ls"
alias la="ls -a"
alias ll="ls -la"

# git
alias g="git"
alias gs="git status"
alias gc="git commit -m"
alias ga="git add"
alias gpl="git pull --rebase"
alias gps="git push"
alias gco="git checkout"

# edit
alias vi="nvim"
alias vim="nvim"
alias csv="csvlens"


# dev
alias do="docker"
alias dc="docker compose"
alias k="kubectl"
alias tf="terraform"
alias python="python3"
alias py="python3"
alias pip-"pip3"

alias dots="/usr/bin/git --git-dir=$HOME/.dotfiles --work-tree=$HOME" 
alias editz="nvim ~/.bashrc"
alias edita="nvim ~/.bash_aliases"
alias editv="nvim ~/.config/nvim/core/plugins.lua"
alias editc="cd ~/.config"
